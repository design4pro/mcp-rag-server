        "--rm",
        "--name",
        "mcp-rag-default",
        "-p",
        "8001:8000",
        "-e",
        "MCP_GEMINI_API_KEY=AIzaSyBydSC6F2F2IEP_PWXsNljkKOqtOfrwj7g",
        "-e",
        "MCP_COLLECTION=default",
        "-e",
        "MCP_PROJECT_NAMESPACE=default",
        "-e",
        "MCP_USER_ID=default",
        "-e",
        "MCP_SERVER_PORT=8000",
        "-e",
        "MCP_LOG_LEVEL=INFO",
        "-e",
        "MCP_QDRANT_URL=http://host.docker.internal:6333",
        "-e",
        "MCP_MEM0_STORAGE_PATH=/app/mem0_data",
        "-v",
        "mcp_rag_mem0_data_default:/app/mem0_data",
        "ghcr.io/design4pro/mcp-rag-server:latest"
